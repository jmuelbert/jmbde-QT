{
  "code-runner.executorMapByFileExtension": {
    ".qml": "qml",
    ".qrc": "qtcreator -client",
    ".ui": "qtcreator -client"
  },
  "files.associations": {
    "*.ts": "xml"
  },

  "yaml.schemas": {
    "https://raw.githubusercontent.com/TingPing/flatpak-manifest-schema/master/flatpak-manifest.schema": [
      "com.github.Desktop.yaml"
    ]
  },
  "python.pythonPath": "/usr/local/Caskroom/miniconda/base/envs/develop/bin/python",
  "esbonio.server.enabled": true,
  "python.formatting.provider": "black"
}
