{
  "code-runner.executorMapByFileExtension": {
    ".qml": "qml",
    ".qrc": "qtcreator -client",
    ".ui": "qtcreator -client"
  },
  "files.associations": {
    "*.ts": "xml"
  },

  "yaml.schemas": {
    "https://raw.githubusercontent.com/TingPing/flatpak-manifest-schema/master/flatpak-manifest.schema": [
      "com.github.Desktop.yaml"
    ]
  },
  "esbonio.server.enabled": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "explorerExclude.backup": {}
}
