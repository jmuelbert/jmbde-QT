include($QT_INSTALL_DOCS/global/qt-cpp-defines.qdocconf)
include($QT_INSTALL_DOCS/global/compat.qdocconf)
include($QT_INSTALL_DOCS/global/fileextensions.qdocconf)

project     = "jmbde"
description = "jmbde Manual"

headerdirs =
sourcedirs = ..
imagedirs = ../images ../templates/images
exampledirs = ..

include(macros.qdocconf)
sources.fileextensions  = "*.qdoc"


qhp.projects            = jmbde
qhp.Qbs.file             = jmbde.qhp
qhp.Qbs.namespace        = de.juergen-muelbert.jmbde.$APP_VERSION_TAG
qhp.Qbs.virtualFolder    = doc
qhp.Qbs.indexTitle       = Qbs
qhp.Qbs.filterAttributes = jmbde $APP_VERSION
qhp.Qbs.customFilters.Qbs.name = jmbde $APP_VERSION
qhp.Qbs.customFilters.Qbs.filterAttributes = jmbde $APP_VERSION
qhp.Qbs.indexRoot        =

qhp.Qbs.subprojects = manual
qhp.Qbs.subprojects.manual.title = jmbde Manual
qhp.Qbs.subprojects.manual.indexTitle = jmbde Manual
qhp.Qbs.subprojects.manual.type = manual

# Doxygen compatibility commands
macro.see                       = "\\sa"
macro.function                  = "\\fn"

navigation.homepage = "jmbde Manual"
buildversion        = "jmbde $APP_VERSION"
